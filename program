    'Headache': Headache,
    'Fever': Fever,
    'Cough': Cough,
    'Redspot': RedSpot,
    'EyeInflame': EyeInflam,
}

SecondDiseaseName = [name for name, value in FirstDiseaseName.items() if value == "1"]
print(SecondDiseaseName)

